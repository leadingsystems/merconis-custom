{
  "name": "leadingsystems/merconis-custom",
  "description": "Leading Systems API as a Contao 4 bundle",
  "keywords": [
    "merconis",
    "leading systems"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.0.0",
    "contao/core-bundle": "~4.4",
    "leadingsystems/contao-merconis": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\ApiBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\ApiBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
